<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox maxWidth="320.0" minWidth="320.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kitchenreceiptprint.controller.ConfigController">
   <Separator prefWidth="200.0">
      <VBox.margin>
         <Insets bottom="10.0" top="10.0" />
      </VBox.margin>
      <contextMenu>
         <ContextMenu>
           <items>
             <MenuItem mnemonicParsing="false" text="Unspecified Action" />
           </items>
         </ContextMenu>
      </contextMenu>
   </Separator>
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="%configftp" textAlignment="CENTER" wrappingWidth="280.0">
      <VBox.margin>
         <Insets bottom="5.0" />
      </VBox.margin>
      <font>
         <Font name="System Bold" size="12.0" />
      </font></Text>
    <Text text="%ftpserver" textAlignment="CENTER">
      <VBox.margin>
         <Insets bottom="5.0" />
      </VBox.margin>
    </Text>
    <TextField fx:id="serverField">
      <VBox.margin>
         <Insets bottom="10.0" />
      </VBox.margin></TextField>
    <Text text="%ftpusername">
      <VBox.margin>
         <Insets bottom="5.0" />
      </VBox.margin></Text>
    <TextField fx:id="usernameField">
      <VBox.margin>
         <Insets bottom="10.0" />
      </VBox.margin></TextField>
    <Text text="%ftpassword">
      <VBox.margin>
         <Insets bottom="5.0" />
      </VBox.margin></Text>
    <PasswordField fx:id="passwordField">
      <VBox.margin>
         <Insets bottom="10.0" />
      </VBox.margin></PasswordField>
    <Text text="%interval">
      <VBox.margin>
         <Insets bottom="5.0" />
      </VBox.margin></Text>
    <TextField fx:id="intervalField">
      <VBox.margin>
         <Insets bottom="5.0" />
      </VBox.margin></TextField>
   <Separator prefWidth="200.0">
      <VBox.margin>
         <Insets bottom="10.0" top="10.0" />
      </VBox.margin>
   </Separator>
   <Text layoutX="30.0" layoutY="72.0" strokeType="OUTSIDE" strokeWidth="0.0" text="%printers" textAlignment="CENTER" wrappingWidth="280.0">
      <font>
         <Font name="System Bold" size="12.0" />
      </font>
      <VBox.margin>
         <Insets bottom="5.0" />
      </VBox.margin>
   </Text>
    <ListView fx:id="printerList" maxHeight="200.0">
      <VBox.margin>
         <Insets bottom="10.0" />
      </VBox.margin></ListView>
    <Button fx:id="save" onAction="#saveConfiguration" text="%confirm" />
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
